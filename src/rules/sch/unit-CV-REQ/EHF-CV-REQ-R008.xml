<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="ehf-cv-request-1.0">
	<assert>
		<description>Verify project value</description>
		<scope>EHF-CV-REQ-R008</scope>
	</assert>

	<test id="1">
		<assert>
			<success>EHF-CV-REQ-R008</success>
		</assert>
		<CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <ProjectExperience>
        <ExperienceValue>
          <ProjectValue>100000</ProjectValue>
        </ExperienceValue>
      </ProjectExperience>

		</CVRequest>
	</test>

  <test id="2">
    <assert>
      <error>EHF-CV-REQ-R008</error>
    </assert>
    <CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <ProjectExperience>
        <ExperienceValue>
          <ProjectValue>0</ProjectValue>
        </ExperienceValue>
      </ProjectExperience>

    </CVRequest>
  </test>

  <test id="3">
    <assert>
      <error>EHF-CV-REQ-R008</error>
    </assert>
    <CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <ProjectExperience>
        <ExperienceValue>
          <ProjectValue>-10000</ProjectValue>
        </ExperienceValue>
      </ProjectExperience>

    </CVRequest>
  </test>

</testSet>

<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="ehf-cv-1.0">
	<assert>
		<description>Verify position percentage</description>
		<scope>EHF-CV-R004</scope>
	</assert>

	<test id="1">
		<assert>
			<success>EHF-CV-R004</success>
		</assert>
		<CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

			<ProjectExperience>
        <ExperienceDescription>
          <PositionPercent>100</PositionPercent>
        </ExperienceDescription>
			</ProjectExperience>

		</CV>
	</test>

	<test id="2">
		<assert>
			<success>EHF-CV-R004</success>
		</assert>
		<CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

			<WorkExperience>
				<WorkExperienceDescription>
					<PositionPercent>100</PositionPercent>
				</WorkExperienceDescription>
			</WorkExperience>

		</CV>
	</test>

  <test id="3">
		<assert>
			<success>EHF-CV-R004</success>
		</assert>
		<CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

	    <ProjectExperience>
	      <ExperienceDescription>
	        <PositionPercent>0</PositionPercent>
	      </ExperienceDescription>
	    </ProjectExperience>

		</CV>
	</test>

	<test id="4">
		<assert>
			<success>EHF-CV-R004</success>
		</assert>
		<CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

			<WorkExperience>
				<WorkExperienceDescription>
					<PositionPercent>0</PositionPercent>
				</WorkExperienceDescription>
			</WorkExperience>

		</CV>
	</test>

  <test id="5">
    <assert>
      <error>EHF-CV-R004</error>
    </assert>
    <CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

      <ProjectExperience>
        <ExperienceDescription>
          <PositionPercent>-10</PositionPercent>
        </ExperienceDescription>
      </ProjectExperience>

    </CV>
  </test>

	<test id="6">
		<assert>
			<error>EHF-CV-R004</error>
		</assert>
		<CV xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CV-1">

			<WorkExperience>
				<WorkExperienceDescription>
					<PositionPercent>-10</PositionPercent>
				</WorkExperienceDescription>
			</WorkExperience>

		</CV>
	</test>

</testSet>

<?xml version="1.0" encoding="UTF-8"?>
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="ehf-cv-request-1.0">
	<assert>
		<description>Verify skill type or title is specified</description>
		<scope>EHF-CV-REQ-R011</scope>
	</assert>

	<test id="1">
		<assert>
			<success>EHF-CV-REQ-R011</success>
		</assert>
		<CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <Skills>
        <Type>tableau</Type>
      </Skills>

		</CVRequest>
	</test>

	<test id="2">
		<assert>
			<success>EHF-CV-REQ-R011</success>
		</assert>
		<CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

			<Skills>
				<AlternativeSkills>
					<Type>tableau</Type>
				</AlternativeSkills>
			</Skills>

		</CVRequest>
	</test>

  <test id="3">
    <assert>
      <success>EHF-CV-REQ-R011</success>
    </assert>
    <CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <Skills>
        <Title>Database development</Title>
      </Skills>

    </CVRequest>
  </test>

	<test id="4">
		<assert>
			<success>EHF-CV-REQ-R011</success>
		</assert>
		<CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

			<Skills>
				<AlternativeSkills>
					<Title>Database development</Title>
				</AlternativeSkills>
			</Skills>

		</CVRequest>
	</test>

  <test id="5">
    <assert>
      <error>EHF-CV-REQ-R011</error>
    </assert>
    <CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

      <Skills>
        <Title>Database development</Title>
        <Type>tableau</Type>
      </Skills>

    </CVRequest>
  </test>

	<test id="6">
		<assert>
			<error>EHF-CV-REQ-R011</error>
		</assert>
		<CVRequest xmlns="urn:fdc:anskaffelser.no:2019:ehf:schema:CVRequest-1">

			<Skills>
				<AlternativeSkills>
					<Title>Database development</Title>
	        <Type>tableau</Type>
				</AlternativeSkills>
			</Skills>

		</CVRequest>
	</test>

</testSet>
